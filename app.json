{
  "expo": {
    "name": "brazof",
    "slug": "brazof",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "brazof",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NFCReaderUsageDescription": "Esta app utiliza NFC para leer etiquetas compatibles.",
        "NSCameraUsageDescription": "Esta app necesita acceso a la cámara.",
        "NSMicrophoneUsageDescription": "Esta app necesita acceso al micrófono.",
        "NSLocationWhenInUseUsageDescription": "Esta app utiliza tu ubicación mientras está en uso.",
        "NSLocationAlwaysUsageDescription": "Esta app necesita tu ubicación incluso cuando está en segundo plano.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.raul302.brazof"
    },
    "android": {
      "permissions": [
        "NFC",
        "android.permission.NFC"
      ],
      "usesFeature": [
        {
          "name": "android.hardware.nfc",
          "required": true
        }
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.raul302.brazof"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "react-native-nfc-manager"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8e224a18-de30-44a2-8508-22797db7d596"
      }
    },
    "owner": "soysho"
  }
}
